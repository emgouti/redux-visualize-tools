{
  "extends": ["tslint:latest", "tslint-config-prettier"],
  "rules": {
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-unused-variable": true,
    "no-console": false,
    "no-empty-interface": false,
    "prefer-for-of": false,
    "no-shadowed-variable": false,
    "no-submodule-imports": false
  }
}
